<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@id/com_appboy_inline_image_push_layout" android:layout_width="fill_parent" android:layout_height="fill_parent" style="@style/Appboy.Push.InlineImage.Layout"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <LinearLayout android:id="@id/com_appboy_inline_image_push_text_area" style="@style/Appboy.Push.InlineImage.TextArea.Layout">
        <LinearLayout android:id="@id/com_appboy_inline_image_push_text_area_header_layout" style="@style/Appboy.Push.InlineImage.TextArea.Header.Layout">
            <ImageView android:id="@id/com_appboy_inline_image_push_app_icon" style="@style/Appboy.Push.InlineImage.TextArea.Header.AppIcon" />
            <TextView android:id="@id/com_appboy_inline_image_push_app_name_text" style="@style/Appboy.Push.InlineImage.TextArea.Header.AppNameText" />
            <TextView android:id="@id/com_appboy_inline_image_push_header_text_divider" style="@style/Appboy.Push.InlineImage.TextArea.Header.TextDivider" />
            <TextView android:id="@id/com_appboy_inline_image_push_time_text" style="@style/Appboy.Push.InlineImage.TextArea.Header.TimeText" />
        </LinearLayout>
        <LinearLayout android:id="@id/com_appboy_inline_image_title_content_layout" style="@style/Appboy.Push.InlineImage.TextArea.TitleContent.Layout">
            <TextView android:id="@id/com_appboy_inline_image_push_title_text" style="@style/Appboy.Push.InlineImage.TextArea.TitleContent.TitleText" />
            <TextView android:id="@id/com_appboy_inline_image_push_content_text" style="@style/Appboy.Push.InlineImage.TextArea.TitleContent.ContentText" />
        </LinearLayout>
    </LinearLayout>
    <ImageView android:id="@id/com_appboy_inline_image_push_side_image" style="@style/Appboy.Push.InlineImage.Image.ImageContent" />
</LinearLayout>